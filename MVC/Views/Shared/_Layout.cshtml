<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>

    @await Html.PartialAsync("./Libraries/HomeLibraries/Styles")

</head>
<body>
<header>
    @await Html.PartialAsync("./Components/HomeComponents/Header")
</header>

<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

<footer class="border-top footer text-muted">
        @await Html.PartialAsync("./Components/HomeComponents/Footer")
</footer>


    @await Html.PartialAsync("./Libraries/HomeLibraries/Scripts")
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>